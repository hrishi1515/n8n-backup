        "jsonBody": "{\"query\": \"query { products(filter: { category_id: { eq: \\\"4\\\" } }, pageSize: 5, currentPage: 1) { total_count items { id sku name url_key price_range { minimum_price { final_price { value currency } } } } } }\"}",
